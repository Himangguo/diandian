<view class='contain'>
  <view class="table">
    <view class='tr'>
    <view class='th'>序号</view>
    <view class='th'>昵称</view>
    <view class='th'>备注</view>
    </view>
    <block wx:for="{{allPerson}}" wx:key="{{item.id}}">
      <view class="tr">
        <view class='td'>{{index+1}}</view>
        <view class="td">{{item.nickname}}</view>
        <view class='td'>{{item.lists.remarkname}}</view>
      </view>
    </block>
  </view>

</view>